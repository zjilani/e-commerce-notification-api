{
    "server" : {
        "port" : 5000,
        "logLevel": "info"
    },
    "smsProviders":{
        "twilio" : {
            "baseURL" : "https://api.twilio.com",
            "apiVersion" : "2010-04-01",
            "senderName" : "Jilani",
            "mode" : "Number",
            "endpoint" : "/Messages.json",
            "accountSID" : "$ACCOUNT_SID",
            "authToken" : "$AUTH_TOKEN",
            "fromNumber" : "$FROM_NUMBER"
        }
    },
    "emailProviders" : {
        "email": {
            "server": "gmail",
            "port": 587,
            "senderName" : "Jilani",
            "senderEmail" : "$EMAIL",
            "username" : "$EMAIL",
            "apiKey" : "$API_KEY"

        }
    },
    "maxPaginationSize":"20",
    "environment":"$ENVIRONMENT",
    "mongodb" : {
        "baseURL" : "$BASE_URL",
        "dbName" : "CUSTOMER",
        "debug" : "false",
        "username" : "$MONGODB_USER",
        "password" : "$MONGODB_PASSWORD",
        "poolSize" : 10
    }
}
